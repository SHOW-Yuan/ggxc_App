<view class="box"><view class="form-box"><view class="carHao"><text>车牌号：</text><input type="text" placeholder="请输入车牌号" data-event-opts="{{[['input',[['__set_model',['','con','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{con}}" bindinput="__e" bindblur="__e"/><view data-ref="singleDom" hidden="{{!(flag)}}" class="sos-box vue-ref"><view class="soso _ul"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['gotoItem',['$0'],[[['items','',index]]]]]]]}}" bindtap="__e" class="_li">{{''+item.cphm+''}}</view></block></block></view></view></view><view><text>教练名称：</text><input type="text" placeholder="请输入教练名称" data-event-opts="{{[['input',[['__set_model',['$0','coach','$event',[]],['formData.datas.__$n0']]]]]}}" value="{{formData.datas[0].coach}}" bindinput="__e"/></view><view><text>选择加燃时间：</text><view style="width:74%;margin:auto;"><xfl-select vue-id="1" list="{{list}}" initValue="汽油" isCanInput="{{false}}" clearable="{{false}}" style_Container="{{listBoxStyle}}" data-event-opts="{{[['^change',[['selectChange']]]]}}" bind:change="__e" bind:__l="__l"></xfl-select></view></view><view><text>加油量/L：</text><input type="number" clearBtn="{{true}}" disabled="{{false}}" placeholder="请输入加油量" data-event-opts="{{[['input',[['__set_model',['','FuelingCapacity','$event',[]]]]]]}}" value="{{FuelingCapacity}}" bindinput="__e"/></view><view><text>燃价：</text><input type="number" clearBtn="{{true}}" disabled="{{false}}" placeholder="请输入油价" data-event-opts="{{[['input',[['__set_model',['','OilPrice','$event',[]]]]]]}}" value="{{OilPrice}}" bindinput="__e"/></view><view><text>加油金额：</text><input disabled type="number" placeholder="加油金额" data-event-opts="{{[['input',[['__set_model',['$0','sum','$event',[]],['formData.datas.__$n0']]]]]}}" value="{{formData.datas[0].sum}}" bindinput="__e"/></view><view><text>选择加燃时间</text><view><uni-calendar class="vue-ref" vue-id="2" insert="{{false}}" data-ref="calendar" value="{{formData.datas[0].ReportingTime}}" data-event-opts="{{[['^confirm',[['confirm']]],['^input',[['__set_model',['$0','ReportingTime','$event',[]],['formData.datas.__$n0']]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l"></uni-calendar><text data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="dakai" bindtap="__e">{{isCalendar?'打开日历':calendarDate}}</text></view></view></view><view class="subBtn-box"><button data-event-opts="{{[['tap',[['service']]]]}}" bindtap="__e">提交</button></view></view>