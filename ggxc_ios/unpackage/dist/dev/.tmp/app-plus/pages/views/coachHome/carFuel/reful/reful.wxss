
page{
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.box {
	padding: 30rpx 0;
}
.form-box>view {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 28rpx;
	padding: 30rpx 10rpx;
	border-bottom: 2rpx solid #E9E9E9;
}
.form-box>view text {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.form-box>view input,
.form-box>view .dakai{
	width: 74%;
	font-size: 28rpx;
}
.form-box .lash {
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	    -ms-flex-align: start;
	        align-items: flex-start;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	border: none;
}
.form-box .lash textarea{
	width: 100%;
	font-size: 28rpx;
	border: 2rpx solid #ccc;
	border-radius: 10rpx;
	height: 180rpx;
	margin-top: 20rpx;
	padding: 14rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.subBtn-box {
	padding: 30rpx 0;
}
.subBtn-box button{
	background-color: #00BB99;
	/* border-radius: 60upx; */
	color: #FFFFFF;
}
/* 搜搜 */
.carHao {
	position: relative;
}
.carHao .sos-box {
	position: absolute;
	left: 50%;
	top: 120rpx;
	-webkit-transform: translate(-50%);
	    -ms-transform: translate(-50%);
	        transform: translate(-50%);
	background: #fff;
	font-size: 28rpx;
	text-align: center;
	border-radius: 10rpx;
	border: 1px solid #dcdfe6;
	-webkit-box-shadow: 0 4rpx 24rpx 0 rgba(0, 0, 0, 0.1);
	        box-shadow: 0 4rpx 24rpx 0 rgba(0, 0, 0, 0.1);
	padding: 10rpx 0;
	width: 50%;
	z-index: 99;
}
.carHao .sos-box::before {
	content: '';
	position: absolute;
	width: 10px;
	height: 10px;
	border-top: 1px solid #ccc;
	border-left: 1px solid #ccc;
	-webkit-transform: rotate(45deg);
	    -ms-transform: rotate(45deg);
	        transform: rotate(45deg);
	top: -6px;
	left: 20%;
	background: #fff;
	z-index: 99999999999;
}
.carHao .soso{
	padding: 0;
	list-style: none;
	max-height: 300px;
	overflow: scroll;
}
.carHao .soso>._li{
   color: #00BB99;
   font-weight: 700;
   padding:20rpx 0;
}
.carHao .soso>._li:hover{
  background:#ECECEC;
}

